{
  "name": "moonshot-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start-trace": "node --trace-warnings node_modules/.bin/next",
    "lint": "next lint && npm run tsc",
    "lint:fix": "next lint --fix",
    "lint-html-report": "next lint -f html -o eslint-report.html",
    "lint-json-report": "next lint -f json -o eslint-report.json",
    "lint-check": "next lint",
    "format-check": "prettier --check .",
    "tsc": "tsc --noEmit",
    "test": "jest",
    "coverage": "npx jest --silent",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.0.1",
    "clsx": "2.0.0",
    "formik": "2.4.5",
    "next": "14.2.3",
    "react": "18",
    "react-dom": "18",
    "react-redux": "9.0.4",
    "react-select": "5.8.0",
    "react-window": "1.8.10",
    "yup": "1.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@jackfranklin/test-data-bot": "^2.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-virtualized": "^9.21.30",
    "@types/react-window": "^1.8.8",
    "@types/redux-logger": "^3.0.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.2.4",
    "prettier-2": "npm:prettier@^2",
    "redux-logger": "^3.0.6",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
