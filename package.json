{
  "name": "moonshot-ui",
  "version": "0.7.4",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start-trace": "node --trace-warnings node_modules/.bin/next",
    "lint": "next lint && npm run tsc",
    "lint:fix": "next lint --fix",
    "lint-html-report": "next lint -f html -o eslint-report.html",
    "lint-json-report": "next lint -f json -o eslint-report.json",
    "lint-check": "next lint",
    "format-check": "prettier --check .",
    "tsc": "tsc --noEmit",
    "test": "jest",
    "coverage": "npx jest --silent",
    "test:watch": "jest --watch --coverageReporters=text --coverageReporters=text-summary --verbose",
    "test:coverage": "jest --coverage",
    "test:cov:dev": "jest --coverage --coverageReporters=text --coverageReporters=text-summary"
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@reduxjs/toolkit": "2.0.1",
    "canvg": "^3.0.11",
    "clsx": "2.0.0",
    "cross-fetch": "^4.1.0",
    "formik": "2.4.5",
    "html2pdf.js": "^0.10.3",
    "next": "^14.2.28",
    "react": "18",
    "react-dom": "18",
    "react-redux": "9.0.4",
    "react-select": "5.8.0",
    "tailwind-merge": "^2.5.4",
    "yup": "1.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@jackfranklin/test-data-bot": "^2.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.30",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "^15.3.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporter": "^3.4.0",
    "jest-json-reporter": "^1.2.2",
    "postcss": "^8",
    "prettier": "^3.2.4",
    "prettier-2": "npm:prettier@^2",
    "redux-logger": "^3.0.6",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.2",
    "typescript": "5.8"
  }
}
